<toolSet name="External Tools">
  <tool name="Robot - QA - Single" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="$PyInterpreterDirectory$/robot" />
      <option name="PARAMETERS" value="-d results_qa_1 -v env:qa_local -v browser:headlesschrome -t &quot;*$SelectedText$*&quot; &quot;$FilePath$&quot;" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  <tool name="Robot - QA - Suite" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="$PyInterpreterDirectory$/robot" />
      <option name="PARAMETERS" value="-d results_qa_a_1 -v env:qa_local -v browser:headlesschrome -e not_ready &quot;$FilePath$&quot;" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
  <tool name="Robot - Dev - Single" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="$PyInterpreterDirectory$/robot" />
      <option name="PARAMETERS" value="-d results_d2 -v env:dev_local -v browser:headlesschrome -t &quot;*$SelectedText$*&quot; &quot;$FilePath$&quot;" />
      <option name="WORKING_DIRECTORY" value="$ProjectFileDir$" />
    </exec>
  </tool>
</toolSet>